    // mqtt HOST
    string mqttHost = 17;

    // mqtt USERNAME
    string mqttUsername = 18;
    
    // mqtt PASSWORD
    string mqttPassword = 19;
    
    // mqtt MQTT_SUBSCRIPTION_TOPIC
    string mqttSubScriptionTopic = 20;

    // 数据库启用
    bool db = 21;

    // 数据库最大连接数
    int32 dbMax = 22;

    // 数据库类型mysql/oracle
    string dbType = 23;

    // 数据库连接字符串
    string dbUrl = 24;

    // 数据库用户名
    string dbUser = 25;

    // 数据库密码
    string dbPassword = 26;

    // 云服务启用
