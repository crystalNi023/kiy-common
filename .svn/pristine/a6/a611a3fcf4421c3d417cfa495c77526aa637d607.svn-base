    // 云服务重置间隔(当连接中断/异常)秒
    int32 cloudReset = 30;

    // 云服务标识
    string cloudId = 31;
    
    // 天气城市ID
    string weatherWeaid = 32;
    
    // PM城市ID
    string aqiWeaid = 33;
    
    // 短信服务
    bool sms = 34; 
    
    // 业主手机号
	string phone = 35;
	
	// 摄像头APP ID
	string cameraAppId = 36;
	
	//摄像头APP SECRET
	string cameraAppSecret = 37;
	
	//摄像头请求Host地址
	string cameraHostAdress = 38;

}

message GetWeatherData {
	// 返回码
	string success = 1;
	
	// weaid
	string weaid = 2;
	
	// 日期
	string days = 3;
	
	// 星期
	string week = 4;
	
	// 城市拼音
	string cityno = 5;
	
	// 城市汉字
	string citynm = 6;
	
	// 城市编号
	string cityid = 7;
	
	// 当日温度区间
	string temperature = 8;
	
	// 当前温度
	string temperature_curr = 9;
	
	// 湿度
	string humidity = 10;
	
	// pm2.5
	string aqi = 11;
	
	// aqi指数分析
	string aqiText = 12;
	
	// 天气
	string weather = 13;
	
	// 气象图标
	string weather_icon = 14;
	
	// 无意义不必理会
	string weather_icon1 = 15;
	
	// 风向
	string wind = 16;
	
	// 风力
	string winp = 17;
	
	// 最高温度
	string temp_high = 18;
	
	// 最低温度
	string temp_low = 19;
	
	// 最大湿度
	string humi_high = 20;
	
	// 最小湿度
	string humi_low = 21;
	
	// 天气ID
	string weatid = 22;
	
	// 无意义不必理会
	string weatid1 = 23;
	
	// 风向ID
	string windid = 24;
	
	// 风力ID
	string winpid = 25;
	
	// 气象图标编号
	string weather_iconid = 26;
}

message UpdateCameraList{
    
     // RSP 数据集
    repeated MDHCamera items = 2;
    
}

message ReadAllDeviceStatus {
    
    // RSP 数据集
    repeated MDeviceStatus item = 1;
}

message GetCameraConfig{

	// 手机号
	string phone = 1;
	// 摄像头APPID
	string appid = 2;
	// 摄像头APP SECRET
	string appsecret = 3;
	// 摄像头请求HOST地址
	string host = 4;
}


